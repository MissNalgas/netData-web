{
	"name": "nextjs-starter",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"dev": "next dev",
		"build": "npm run sw && next build",
		"start": "next start",
		"lint": "next lint --fix",
		"commit": "git cz",
		"pretty": "prettier --write \"src/**/*.{ts,js,scss,css,html,json}\"",
		"test": "jest --watch",
		"sw": "browserify -t [ babelify --presets [ @babel/preset-env ] --global ] sw/index.js -o public/firebase-messaging-sw.js"
	},
	"dependencies": {
		"@hookform/resolvers": "^3.3.2",
		"@reduxjs/toolkit": "^1.9.7",
		"axios": "^1.6.0",
		"browserify": "^17.0.0",
		"echarts": "^5.4.3",
		"facepaint": "^1.2.1",
		"firebase": "^10.6.0",
		"icomoon-react": "^3.0.0",
		"next": "14.0.0",
		"react": "^18",
		"react-dom": "^18",
		"react-hook-form": "^7.47.0",
		"react-redux": "^8.1.3",
		"redux-persist": "^6.0.0",
		"reselect": "^4.1.8",
		"styled-components": "^6.1.0",
		"yup": "^1.3.2"
	},
	"devDependencies": {
		"@babel/core": "^7.23.3",
		"@babel/preset-env": "^7.23.3",
		"@testing-library/jest-dom": "^6.1.4",
		"@testing-library/react": "^14.0.0",
		"@types/facepaint": "^1.2.4",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"autoprefixer": "^10.4.16",
		"babel-preset-es2015": "^6.24.1",
		"babelify": "^10.0.0",
		"commitizen": "^4.3.0",
		"cz-customizable": "^7.0.0",
		"eslint": "^8",
		"eslint-config-next": "14.0.0",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"postcss": "^8.4.31",
		"prettier": "3.0.3",
		"tailwindcss": "^3.3.5",
		"typescript": "^5"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "npm run lint",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
